<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/wallpaper"
    tools:context=".MainActivity">
<!--P-->

    <!--leme-->

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_marginStart="45dp"
        android:layout_marginTop="240dp"
        android:rotationX="180"
        android:src="@drawable/good"
        app:layout_constraintBottom_toTopOf="@+id/Main_title2"
        app:layout_constraintEnd_toStartOf="@+id/Main_title"
        app:layout_constraintHorizontal_bias="0.316"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.602" />

    <ImageView
        android:id="@+id/Main_title"
        android:layout_width="161dp"
        android:layout_height="72dp"
        android:layout_marginEnd="85dp"
        android:layout_marginBottom="44dp"
        android:src="@drawable/leme"
        android:textColor="#FEFEFE"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="@+id/Main_title2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/Main_title2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="67dp"
        android:layout_marginEnd="67dp"
        android:fontFamily="@font/bemin_1ttf"
        android:text="터치시 다음으로 넘어갑니다"
        android:textColor="#D8DFE5"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>